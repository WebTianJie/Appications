.container{
  padding: 0;
  position: relative;
  width:100%;
  height:100%;
}
page{
  height:100%;
}
.canvas-area{
  width:100%;
  height:100%;
  background: #f8f8f8;
}
.myCanvas{
  width:100%;
  height:100%;
  background: #ddd;
}
.canvas-tools{
  position:absolute;
  left:0;
  bottom:20rpx;
  width:100%;
  height:100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.box{
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  background: purple;
}
.box1{
  background:#9ccc;
}
.box2{
  background: #9ccc;
}
.box3{
  background:yellow;
}
.box4{
  background:red;
}
.box5{
  background:white;
}