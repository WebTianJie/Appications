.container{
  width:100%;
  height:100%;
  background: rgb(136, 136, 218);
}
.page-area{
  width:90%;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding:30rpx 0;
}
.logo{
  width:150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.page-section{
  display: flex;
flex-direction: column;
justify-content: flex-start;
}
.name{
  font-size:60rpx;
}