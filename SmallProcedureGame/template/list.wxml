<template name="listTemp">
  <view class="list" wx:for="{{gameArr}}" wx:key="{{item}}">
        <image class="list-img" src="{{item.imgSrc}}"></image>
        <view class="list-font">
            <view class="title">{{item.title}}</view>
            <text class="follow">关注</text>
            <text class="try-play">试玩</text>
        </view>
        <button size="mini" class="list-btn">下载</button>
  </view>
</template>